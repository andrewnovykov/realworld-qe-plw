{
	"extends": ["eslint:recommended", "plugin:prettier/recommended"],
	"parserOptions": {
		"ecmaVersion": 2021,
		"sourceType": "module"
	},
	"env": {
		"es2021": true,
		"node": true,
		"browser": true
	},
	"rules": {
		"prettier/prettier": "error",
		"max-len": ["error", { "code": 120, "ignoreUrls": true }]
	}
}
